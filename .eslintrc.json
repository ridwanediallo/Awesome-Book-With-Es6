{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"extends": ["airbnb-base"],
	"rules": {
		"no-shadow": "off",
		"no-param-reassign": "off",
		"eol-last": "off",
		"max-classes-per-file": "off",
		"no-tabs": 0,
		"no-mixed-spaces-and-tabs": 0,
		"import/extensions": [
			1,
			{
				"js": "always",
				"json": "always"
			}
		]


	},
	"ignorePatterns": ["dist/", "build/"]


}